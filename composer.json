{
  "name": "theoaks/relative-datetime-format-builder",
  "description": "Object Oriented approach to building the different relative date/time formats that the strtotime(), DateTime and date_create() parser understands.",
  "type": "library",
  "homepage": "https://github.com/theoaks/relative-datetime-format-builder",
  "license": "GNU",
  "keywords": ["datetime", "formating", "strtotime"],
  "autoload": {
    "classmap": [
      "src/"
    ],
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Oaks\\RelativeDatetimeFormatBuilder\\": "src/",
      "Oaks\\RelativeDatetimeFormatBuilder\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Babatunde Ojerinde",
      "email": "ojerindej@theoaksoft.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "webmozart/assert": "^1.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
